                yearDiv.innerText = "0" + yearDiv.innerText.substr(1, 4)
                yearCounter --
                if(yearCounter <= 0) yearCounter = 4
              } else if(yearCounter % 3 === 0) {
                yearDiv.innerText = yearDiv.innerText.substr(0, 1) + "0" + yearDiv.innerText.substr(2, 4)
                yearCounter --
                if(yearCounter <= 0) yearCounter = 4
              } else if(yearCounter % 2 === 0) {
                yearDiv.innerText = yearDiv.innerText.substr(0, 2) + "0" + yearDiv.innerText.substr(3, 4)
                yearCounter --
                if(yearCounter <= 0) yearCounter = 4
              } else if(yearCounter % 1 === 0) {
                yearDiv.innerText = yearDiv.innerText.substr(0, 3) + "0"
                yearCounter --
                if(yearCounter <= 0) yearCounter = 4
              }
            }
          }

          if(ampmBool) {
            if(e.keyCode === 65) {
              ampmDiv.innerText = "A" + ampmDiv.innerText.substr(1, 2)
            }
            if(e.keyCode === 80) {
              ampmDiv.innerText = "P" + ampmDiv.innerText.substr(1, 2)
            }
          }
        })
      },

      startDatetimeValue: function() {
        let values = []
        for (let i=0; i<startDatetimeArray.length; i++) {
          if(startDatetimeArray[i].innerText !== "/" && startDatetimeArray[i].innerText !== ":") {
            values.push(startDatetimeArray[i].innerText)
          }
        }
        let dateStr = values[2] + " " + months[values[1]] + " " + values[3] + " " + convertTime[values[4]+values[6]] + ":" + values[5] + ":" + "00" + " UTC +00:00"
        return dateStr
      },

      insertValue: function() {
        if(theForm !== null) {
          theForm.addEventListener("submit", e => {
            console.log("here");
            e.preventDefault()
            startDatetimeInput.value = startDatetimeValue()
          })
        }
      }
    }
    return startDateHash
  }

</script>
